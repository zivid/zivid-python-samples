[MESSAGES CONTROL]
disable=consider-using-enumerate,
        import-error,
        missing-docstring,
        no-member,
        duplicate-code,
        no-name-in-module,

[TYPE CHECK]
generated-members=cv2.*

[DESIGN]
max-args=7
max-locals=30
max-returns=9
max-bool-expr=6
max-statements=70
max-branches=17

[FORMAT]
max-line-length=225
